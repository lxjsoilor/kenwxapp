@-webkit-keyframes breathe {
    0% {
        width: 64rpx;
        height: 64rpx;
        opacity: 0;
    }

    5% {
        width: 64rpx;
        height: 64rpx;
        opacity: 0;
        display: block;
    }

    10% {
        width: 64rpx;
        height: 64rpx;
        opacity: 1;
    }

    90% {
        width: 120rpx;
        height: 120rpx;
        opacity: 0;
    }

    95% {
        width: 120rpx;
        height: 120rpx;
        opacity: 0;
        display: none;
    }

    100% {
        width: 120rpx;
        height: 120rpx;
        opacity: 0;
    }
}

