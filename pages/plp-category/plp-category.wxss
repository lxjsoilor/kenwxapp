}

.page-con .big-circle .view-out-li .view-li .text-ch {
    position: absolute;
    top: 46rpx;
    left: 40rpx;
    z-index: 89;
    animation: right-left-short 1.2s linear;
}

.page-con .big-circle .view-out-li .view-li .text-ch .text1 {
    font-size: 48rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: white;
    line-height: 66rpx;
    letter-spacing: 2rpx;
}

.page-con .big-circle .view-out-li .view-li .text-ch .text2 {
    font-size: 24rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: white;
    line-height: 34rpx;
    margin-top: 4rpx;
    letter-spacing: 2rpx;
}

.page-con .big-circle .view-out-li .view-li .text-en {
    position: absolute;
    top: 320rpx;
    left: 40rpx;
    z-index: 89;
    font-size: 150rpx;
    font-family: "Helvetica Neue LT Std";
    font-weight: normal;
    color: rgba(70,103,144,0.16);
    line-height: 156rpx;
    animation: fade-in 1s linear;
}

.page-con .big-circle .view-out-li .view-li .dashed {
    width: 600rpx;
    height: 600rpx;
    position: absolute;
    top: 164rpx;
    right: -450rpx;
    z-index: 990;
    animation: right-left-long 1.5s linear;
}

.page-con .big-circle .view-out-li .view-li .dashed image {
    width: 100%;
    height: 100%;
}

.page-con .big-circle .view-out-li .view-li .dashed .spu-li {
    width: 40rpx;
    height: 100%;
    border-radius: 40rpx;
    position: absolute;
    left: 280rpx;
    top: 0;
    z-index: 991;
}

.page-con .big-circle .view-out-li .view-li .dashed .spu-li .spu-li-img {
    width: 50rpx;
    height: 50rpx;
    position: absolute;
    left: 50%;
    top: 0;
    transform: translate(-50%,-50%);
    z-index: 9999;
    margin: 0 auto;
    border-radius: 50%;
    border: 4rpx solid #fff;
}

.page-con .big-circle .view-out-li .view-li .dashed .spu-li .spu-li-img.checked {
    width: 64rpx;
    height: 64rpx;
    top: 0;
    left: 50%;
    transform: translate(-50%,-50%);
}

.page-con .big-circle .view-out-li .view-li .dashed .spu-li .bg-shadow {
    width: 72rpx;
    height: 72rpx;
    position: absolute;
    left: 50%;
    top: 0rpx;
    z-index: -1;
    transform: translate(-50%,-50%);
    border: 4rpx solid #fff;
    border-radius: 50%;
}

.page-con .big-circle .view-out-li .view-li .dashed .spu-li .bg-shadow:before {
    content: "";
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    border: 4rpx solid #fff;
    border-radius: 50%;
    opacity: 0;
    animation: breathe 2s linear 0s infinite normal;
}

.page-con .big-circle .view-out-li .view-li .dashed .spu-li .bg-shadow::after {
    content: "";
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    border: 4rpx solid #fff;
    border-radius: 50%;
    opacity: 0;
    animation: breathe 2s linear -1s infinite normal;
}

.page-con .big-circle .view-out-li .view-li .slide-image {
    width: 650rpx;
    height: 650rpx;
    position: absolute;
    left: 50%;
    top: 120rpx;
    transform: translateX(-50%);
    z-index: 99;
    opacity: 0;
}

.page-con .big-circle .view-out-li .view-li .slide-image.show {
    opacity: 1;
}

.page-con .big-circle .view-out-li .view-li .slide-image.hide {
    opacity: 1;
    animation: hide 0.8s ease-out;
    opacity: 0;
}

.page-con .big-circle .view-out-li .view-li .slide-image-shadow {
    width: 300rpx;
    height: 144rpx;
    position: absolute;
    left: 50%;
    top: 692rpx;
    transform: translateX(-50%);
    z-index: 99;
}

.page-con .big-circle .view-out-li .view-li .buy-info {
    position: absolute;
    top: 788rpx;
    left: 50%;
    transform: translateX(-50%);
}

.page-con .big-circle .view-out-li .view-li .buy-info .product-info {
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #FFFFFF;
    letter-spacing: 0;
    line-height: 40rpx;
    display: flex;
