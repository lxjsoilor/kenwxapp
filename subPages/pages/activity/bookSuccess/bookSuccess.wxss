.bookSuccess-wrap {
    background-color: #FFFFFF;
    position: relative;
}

.bookSuccess-wrap .BS-banner-w {
    position: relative;
    height: 408rpx;
    width: 100%;
    margin-bottom: 110rpx;
}

.bookSuccess-wrap .BS-title {
    padding: 34rpx 0 66rpx 0;
    text-align: center;
    font-size: 28rpx;
    font-family: PingFangSC-Light;
    font-weight: 300;
    color: #000000;
    line-height: 48rpx;
    margin: 0 70rpx;
    border-bottom: 2rpx dashed #DDDDDD;
}

.bookSuccess-wrap .BS-title .title {
    font-size: 48rpx;
    font-family: PingFangSC-Light;
    font-weight: 300;
    line-height: 36rpx;
    margin-bottom: 40rpx;
}

.bookSuccess-wrap .BS-title .tips {
    font-size: 24rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    line-height: 24rpx;
}

.bookSuccess-wrap .BS-tips {
    margin: 0 70rpx;
    padding-top: 24rpx;
}

.bookSuccess-wrap .BS-tips .tips-item {
    margin-top: 40rpx;
    text-align: center;
    font-size: 24rpx;
    color: #222222;
    line-height: 34rpx;
}

.bookSuccess-wrap .BS-tips .tips-item .item-title {
    font-family: PingFangSC-Light;
    font-weight: 300;
}

.bookSuccess-wrap .BS-tips .tips-item .item-data {
    margin-top: 10rpx;
    font-family: PingFangSC-Medium;
    font-weight: 500;
}

.bookSuccess-wrap .btn-w {
    padding: 0rpx 110rpx;
}

.bookSuccess-wrap .btn-w .goHome-btn {
    margin-top: 40rpx;
    width: 530rpx;
}