.actBook-wrap {
    background-color: #F2F2F2;
    position: relative;
}

.actBook-wrap .actBook-banner-w {
    position: relative;
    height: 1186rpx;
    width: 100%;
    z-index: 9;
}

.actBook-wrap .actBook-banner-w .contact-w {
    width: 80rpx;
    height: 80rpx;
    overflow: hidden;
    border-radius: 100%;
    background-color: #FFFFFF;
    position: absolute;
    right: 20rpx;
    top: 580rpx;
    box-shadow: 0 20rpx 40rpx 0 rgba(0,0,0,0.08);
}

.actBook-wrap .actBook-banner-w .contact-w .contact-btn .icon-service {
    background-color: #FFFFFF;
    line-height: 80rpx;
    font-size: 36rpx;
}

.actBook-wrap .actBook-banner-w .share-btn-w {
    width: 80rpx;
    height: 80rpx;
    overflow: hidden;
    border-radius: 100%;
    background-color: #FFFFFF;
    position: absolute;
    right: 20rpx;
    top: 700rpx;
    box-shadow: 0 20rpx 40rpx 0 rgba(0,0,0,0.08);
}

.actBook-wrap .actBook-banner-w .share-btn-w .share-btn {
    height: 80rpx;
    background-color: #FFFFFF;
}

.actBook-wrap .actBook-banner-w .share-btn-w .share-btn .icon-share {
    line-height: 80rpx;
    font-size: 36rpx;
}

.actBook-wrap .activity-info-w {
    margin: -46rpx 30rpx 0 30rpx;
    background-color: #F2F2F2;
    padding-bottom: 40rpx;
}

.actBook-wrap .activity-info-w .activity-info {
    position: relative;
    width: 690rpx;
    padding: 40rpx;
    z-index: 99;
    background: #FFFFFF;
    box-shadow: 0 4rpx 40rpx 0 rgba(0,0,0,0.08);
    border-radius: 4rpx;
}

.actBook-wrap .activity-info-w .activity-info .end-title {
    text-align: center;
    margin: 6rpx 0 20rpx 0;
    font-size: 32rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: #222222;
    line-height: 44rpx;
    letter-spacing: 2rpx;
}

.actBook-wrap .activity-info-w .activity-info .end-tips {
    text-align: center;
    font-size: 24rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: #A4A3A4;
    line-height: 34rpx;
}

.actBook-wrap .activity-info-w .activity-info .title {
    text-align: center;
    margin: 6rpx 0 60rpx 0;
    font-size: 32rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: #222222;
    line-height: 44rpx;
    letter-spacing: 2rpx;
}

.actBook-wrap .activity-info-w .activity-info .ben-form-group.activityDate .ben-form-label,.actBook-wrap .activity-info-w .activity-info .ben-form-group.userName .ben-form-label {
    color: #000000;
    font-size: 28rpx;
}

.actBook-wrap .activity-info-w .activity-info .ben-form-group .ben-form-label {
    margin-left: 20rpx;
    font-size: 24rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: #A4A3A4;
    line-height: 34rpx;
}

.actBook-wrap .activity-info-w .activity-info .ben-form-group .ben-input-group {
    margin-top: 10rpx;
    border-radius: 4rpx;
}

.actBook-wrap .activity-info-w .activity-info .weChatCode-radio-title {
    margin-left: 20rpx;
    font-size: 24rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: #A4A3A4;
    line-height: 34rpx;
}

.actBook-wrap .activity-info-w .activity-info .weChatCode-radio-group {
    justify-content: flex-start;
    margin: 32rpx 0 12rpx 20rpx;
}

.actBook-wrap .activity-info-w .activity-info .weChatCode-radio-group.phone {
    margin-bottom: 60rpx;
}

.actBook-wrap .activity-info-w .activity-info .weChatCode-radio-group .weChatCode-radio-item {
    justify-content: flex-start;
    width: 160rpx;
    position: relative;
    padding: 20rpx 0 20rpx 48rpx;
    font-size: 26rpx;
    color: #000000;
    line-height: 28rpx;
    margin-right: 80rpx;
}

.actBook-wrap .activity-info-w .activity-info .weChatCode-radio-group .weChatCode-radio-item .label {
    position: absolute;
    left: 0;
}

.actBook-wrap .activity-info-w .activity-info .weChatCode-radio-group .weChatCode-radio-item .label .icon-font {
    color: #DDDDDD;
}

.actBook-wrap .activity-info-w .activity-info .weChatCode-radio-group .weChatCode-radio-item.active .label {
    border: 0;
    text-align: center;
    line-height: 28rpx;
}

.actBook-wrap .activity-info-w .activity-info .weChatCode-radio-group .weChatCode-radio-item.active .label text {
    color: #092DC5;
    display: inline-block;
}

.actBook-wrap .activity-info-w .activity-info .activityInfo-submit {
    margin-bottom: 40rpx;
}

.actBook-wrap .share-card-con {
    width: 100%;
    position: fixed;
    left: 0;
    z-index: 100;
}

.actBook-wrap .share-card-con .bg-mask {
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.6);
}

.actBook-wrap .share-card-con .white-bg {
    width: 100%;
    height: 410rpx;
    position: absolute;
    bottom: 0;
    left: 0;
    background-color: #fff;
}

.actBook-wrap .share-card-con .white-bg .save-share-card {
    width: calc(100% - 80rpx);
    position: absolute;
    left: 40rpx;
    bottom: 16rpx;
    background: #092DC5;
    border-radius: 6rpx;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #FFFFFF;
    letter-spacing: 2rpx;
}

.actBook-wrap .share-card-con .white-bg .save-share-card .icon-font {
    margin-right: 16rpx;
}

.actBook-wrap .share-card-con .white-bg>image {
    width: 550rpx;
    height: 908rpx;
    position: absolute;
    bottom: 168rpx;
    left: 50%;
    transform: translateX(-50%);
    box-shadow: 0 30rpx 60rpx 0 rgba(0,0,0,0.1);
}

.actBook-wrap .share-card-con .white-bg>.cps-img-margin {
