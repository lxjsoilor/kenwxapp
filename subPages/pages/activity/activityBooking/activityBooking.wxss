.actBook-wrap .share-card-con .cps-bg-con .cps-share-btn {
    height: 224rpx;
    width: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
}

.actBook-wrap .share-card-con .cps-bg-con .cps-share-btn .btn-con:nth-child(1) {
    margin-right: 154rpx;
}

.actBook-wrap .share-card-con .cps-bg-con .cps-share-btn .btn-con {
