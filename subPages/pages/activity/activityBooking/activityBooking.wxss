    background-color: #fff;
    padding: 20rpx 20rpx 0;
    text-align: center;
    position: absolute;
}

.actBook-wrap .share-card-con .cps-bg-con image {
    width: 576rpx;
    height: 576rpx;
}

.actBook-wrap .share-card-con .cps-bg-con .icon-close {
    position: absolute;
    bottom: -126rpx;
    left: 50%;
    transform: translateX(-50%);
