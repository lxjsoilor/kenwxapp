@import "../../../__wuBaseWxss__/1.wxss";

button {
    border: none;
    margin: 0;
    padding: 0;
    display: inline-block;
    background-color: rgba(255,255,255,0);
    width: 100%;
    text-align: center;
}

button::after {
    content: '';
}

page {
    width: 100%;
    height: 100vh;
    position: relative;
    overflow-y: auto;
    overflow-x: hidden;
    -webkit-overflow-scrolling: touch;
    box-sizing: border-box;
}

page .page-con {
    width: 100%;
    overflow-y: auto;
    overflow-x: hidden;
    box-sizing: border-box;
    -webkit-overflow-scrolling: touch;
}

page .spell-img-con {
    position: relative;
    margin-bottom: 92rpx;
}

page .spell-img-con .bg-img {
    width: 100%;
    height: 528rpx;
}

page .spell-img-con .logo_img {
    width: 90rpx;
    height: 90rpx;
    position: absolute;
    bottom: -45rpx;
    left: 50%;
    transform: translateX(-50%);
}

page .spell-img-con .spell-rull-con {
    width: 176rpx;
    height: 60rpx;
    line-height: 60rpx;
    background: #FFFFFF;
    box-shadow: 0 8rpx 16rpx 0 rgba(0,0,0,0.06);
    border-radius: 170rpx 0px 0px 170rpx;
    padding: 0 34rpx;
    box-sizing: border-box;
    position: absolute;
    top: 24rpx;
    right: -20rpx;
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #3E3D3E;
}

page .spell-tips {
    font-family: PingFangSC-Regular;
    font-size: 32rpx;
    color: #3E3D3E;
    text-align: center;
    line-height: 46rpx;
    margin-bottom: 12rpx;
}

page .spell-tips .spell-success-tip {
    font-size: 24rpx;
    color: #7E7D7E;
    width: 586rpx;
    margin-left: 50%;
    transform: translateX(-50%);
    margin-top: 14rpx;
}

page .spell-tips .spell-fail-tip {
    margin-left: 50%;
    transform: translateX(-50%);
    width: 488rpx;
}

page .spell-btn {
    background: #092DC5;
    border-radius: 6rpx;
    width: 570rpx;
    text-align: center;
    line-height: 80rpx;
    height: 80rpx;
    margin-left: 50%;
    transform: translateX(-50%);
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #FFFFFF;
    letter-spacing: 2rpx;
}

page .btn-bot-tip {
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #3E3D3E;
    margin: 34rpx 0 32rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

page .btn-bot-tip .icon-font {
    font-size: 16rpx;
    margin-left: 10rpx;
}

page .spell-bot-tip {
    width: calc(100% - 80rpx);
    margin-left: 40rpx;
    border-top: 2rpx dashed #eee;
    padding-top: 30rpx;
    box-sizing: border-box;
    padding-bottom: 20rpx;
}

page .spell-bot-tip>view {
    width: 570rpx;
    margin-left: 50%;
    transform: translateX(-50%);
    display: flex;
    margin-bottom: 8rpx;
    align-items: flex-start;
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #7E7D7E;
}

page .spell-bot-tip>view .spell-circle {
    width: 6rpx;
    height: 6rpx;
    border-radius: 100%;
    background: #3E3D3E;
    margin-right: 18rpx;
    flex-shrink: 0;
    margin-top: 16rpx;
}

page .spell-time-con {
    margin-bottom: 20rpx;
    font-family: PingFangSC-Light;
    font-size: 24rpx;
    color: #3E3D3E;
    letter-spacing: 1.72rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-top: 50rpx;
}

page .spell-time-con .count-num {
    font-family: PublicoBanner-Light;
    font-size: 28rpx;
    color: #092DC5;
    letter-spacing: 1.22rpx;
    text-align: center;
    margin: 0 10rpx;
    width: 240rpx;
}

page .spell-member-tip {
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #3E3D3E;
    text-align: center;
    line-height: 46rpx;
    width: 488rpx;
    position: relative;
    margin-left: 50%;
    transform: translateX(-50%);
}

page .spell-member-tip .icon-font {
    position: absolute;
    top: 0;
    font-size: 30rpx;
    color: #ddd;
}

page .spell-member-tip .icon-yinhao_left {
    left: -72rpx;
}

page .spell-member-tip .icon-yinhao_right {
    right: -72rpx;
}

page .parent-con {
    max-height: 104rpx;
    overflow: hidden;
    transition: max-height .1s;
}

page .parent-con .member-con {
    width: 586rpx;
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    margin-left: 50%;
    transform: translateX(-50%);
    position: relative;
    padding-top: 24rpx;
}

page .parent-con .member-con .member-item {
    width: 80rpx;
    height: 80rpx;
    position: relative;
    margin-right: 20rpx;
    margin-top: 42rpx;
}

page .parent-con .member-con .member-item>image {
    width: 100%;
    height: 100%;
    border-radius: 100%;
}

page .parent-con .member-con .member-item .icon-font {
    position: absolute;
    color: #CBB289;
    left: -3rpx;
    top: -16rpx;
    display: inline-block;
    width: 34rpx;
    height: 28rpx;
    z-index: 2;
}

page .parent-con .member-con .zero-margin {
    margin-top: 0;
}

page .parent-con .member-con .add-item {
    width: 76.8rpx;
    height: 76.8rpx;
    border-radius: 100%;
    text-align: center;
    line-height: 76.8rpx;
    background-color: #efefef;
    color: #c3c3c3;
    margin-right: 20rpx;
    margin-bottom: 42rpx;
}

page .parent-con .member-con .add-item .icon-font {
    font-size: 24rpx;
}

page .mask-slideDown {
    max-height: 100%;
    overflow: hidden;
}

page .slide-btn {
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #3E3D3E;
    text-align: center;
    margin-top: 32rpx;
}

page .slide-btn .icon-font {
    font-size: 16rpx;
    margin-left: 10.8rpx;
    transform: rotate(270deg);
}

page .shouqi-btn {
    margin-top: 24rpx;
}

page .rule-mask {
    width: 100%;
    position: absolute;
    left: 0;
    z-index: 100;
}

page .rule-mask .bg-mask {
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.6);
}

page .rule-mask .pop-center {
    width: calc(100% - 80rpx);
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
}

page .rule-mask .butn-center {
    width: 100%;
    text-align: center;
    margin-top: 30rpx;
}

page .rule-mask .butn-center .icon-close {
    color: #fff;
    font-size: 80rpx;
}

page .rule-mask .pop-up-con {
    width: 100%;
    border-radius: 6rpx;
    background-color: #fff;
    padding: 50rpx 40rpx 44rpx;
    box-sizing: border-box;
}

page .rule-mask .pop-up-con .rule-title {
    text-align: center;
    font-family: PingFangSC-Regular;
    font-size: 36rpx;
    color: #3E3D3E;
    letter-spacing: 1.28rpx;
    margin-bottom: 60rpx;
}

page .rule-mask .pop-up-con .rule-detail {
    height: 760rpx;
    overflow: auto;
    font-family: PingFangSC-Regular;
    font-size: 26rpx;
    color: #7E7D7E;
    line-height: 44rpx;
}

page .rule-mask .pop-up-con .rule-detail .ruletext {
    color: #919191;
    margin-bottom: 20rpx;
}

page .rule-mask .pop-up-con .rule-detail .ruletitle {
    font-size: 28rpx;
    color: #3E3D3E;
    line-height: 44rpx;
    padding-bottom: 30rpx;
}

.pdp-share-con {
    width: 100%;
    position: fixed;
    left: 0;
    z-index: 100;
}

.pdp-share-con .bg-mask {
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.6);
}

.pdp-share-con button {
    width: auto;
}

.pdp-share-con .share-btn-con {
    width: calc(100% - 40rpx);
    margin-left: 20rpx;
    bottom: 20rpx;
    position: absolute;
}

.pdp-share-con .share-btn-con .share-btn {
    width: 100%;
    height: 240rpx;
    background-color: rgba(255,255,255,0.95);
    border-radius: 10rpx;
    margin-bottom: 2rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.pdp-share-con .share-btn-con .share-btn .btn-con:nth-child(1) {
    margin-right: 154rpx;
}

.pdp-share-con .share-btn-con .share-btn .btn-con {
    line-height: inherit;
    background-color: rgba(255,255,255,0);
}

.pdp-share-con .share-btn-con .share-btn .btn-con .icon-font {
    font-size: 60rpx;
    margin-bottom: 14rpx;
    color: #3d3d3d;
}

.pdp-share-con .share-btn-con .share-btn .btn-con>view:nth-child(2) {
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #3E3D3E;
    background-color: rgba(255,255,255,0);
}

.pdp-share-con .share-btn-con .share-true {
    width: 100%;
    height: 88rpx;
    background-color: rgba(255,255,255,0.95);
    border-radius: 10rpx;
    line-height: 88rpx;
    text-align: center;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #092DC5;
    letter-spacing: 2rpx;
}

.share-card-con {
    width: 100%;
    position: fixed;
    left: 0;
    z-index: 100;
}

.share-card-con .bg-mask {
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.6);
}

.share-card-con .white-bg {
    width: 100%;
    height: 410rpx;
    position: absolute;
    bottom: 0;
    left: 0;
    background-color: #fff;
}

.share-card-con .white-bg .save-share-card {
    width: calc(100% - 80rpx);
    position: absolute;
    left: 40rpx;
    bottom: 16rpx;
    background: #092DC5;
    border-radius: 6rpx;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #FFFFFF;
    letter-spacing: 2rpx;
}

.share-card-con .white-bg .save-share-card .icon-font {
    margin-right: 16rpx;
}

.share-card-con .white-bg>image {
    width: 550rpx;
    height: 908rpx;
    position: absolute;
    bottom: 168rpx;
    left: 50%;
    transform: translateX(-50%);
    box-shadow: 0 30rpx 60rpx 0 rgba(0,0,0,0.1);
}

.share-card-con .white-bg>.cps-img-margin {
    bottom: 208rpx;
}

.share-card-con .cps-bg-con {
    width: calc(100% - 132rpx);
    margin-left: 66rpx;
    top: 106rpx;
    background-color: #fff;
    padding: 20rpx 20rpx 0;
    text-align: center;
    position: absolute;
}

.share-card-con .cps-bg-con image {
    width: 576rpx;
    height: 576rpx;
}

.share-card-con .cps-bg-con .icon-close {
    position: absolute;
    bottom: -126rpx;
    left: 50%;
    transform: translateX(-50%);
    font-size: 78rpx;
    color: #fff;
}

.share-card-con .cps-bg-con .cps-share-btn {
    height: 224rpx;
    width: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
}

.share-card-con .cps-bg-con .cps-share-btn .btn-con:nth-child(1) {
    margin-right: 154rpx;
}

.share-card-con .cps-bg-con .cps-share-btn .btn-con {
    line-height: inherit;
    background-color: rgba(255,255,255,0);
}

.share-card-con .cps-bg-con .cps-share-btn .btn-con .icon-font {
    font-size: 60rpx;
    margin-bottom: 14rpx;
    color: #3d3d3d;
}

.share-card-con .cps-bg-con .cps-share-btn .btn-con>view:nth-child(2) {
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #3E3D3E;
    background-color: rgba(255,255,255,0);
}

.isIpx .white-bg {
    height: 470rpx;
}

.isIpx .save-share-card {
    bottom: 60rpx!important;
}

@-webkit-keyframes slideDown {
    to {
        max-height: 160rpx;
    }
}

@keyframes slideDown {
    to {
        max-height: 160rpx;
    }
}

@-webkit-keyframes slideUp {
    to {
        max-height: 80rpx;
    }
}