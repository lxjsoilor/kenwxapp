.orderDetail-wrap {
    width: 100%;
    min-height: 1334rpx;
    background-color: #F2F2F2;
    position: relative;
    padding-bottom: 116rpx;
}

.orderDetail-wrap .space-line {
    width: 100%;
    height: 20rpx;
    background-color: #F2F2F2;
}

.orderDetail-wrap .detail-top-wrap {
    padding: 40rpx 40rpx 40rpx 60rpx;
    position: relative;
    align-items: baseline;
}

.orderDetail-wrap .detail-top-wrap .top-icon-w .icon-font {
    font-size: 40rpx;
}

.orderDetail-wrap .detail-top-wrap .top-data-wrap {
    width: calc(100% - 80rpx);
}

.orderDetail-wrap .detail-top-wrap .top-data-wrap .order-status-txt {
    font-family: PingFangSC-Regular;
    line-height: 56rpx;
    font-size: 40rpx;
    color: #3E3D3E;
    letter-spacing: 0;
}

.orderDetail-wrap .detail-top-wrap .top-data-wrap .order-status-desc {
    font-family: PingFangSC-Regular;
    line-height: 34rpx;
    font-size: 24rpx;
    color: #7E7D7E;
    letter-spacing: 0;
    margin-top: 8rpx;
}

.orderDetail-wrap .detail-top-wrap .top-data-wrap .order-status-desc.time {
    color: #092DC5;
}

.orderDetail-wrap .detail-top-wrap .top-btn-wrap {
    position: absolute;
    right: 40rpx;
}

.orderDetail-wrap .detail-top-wrap .top-btn-wrap button {
    background: #F2F2F2;
    border: 2rpx solid #A4A3A4;
    border-radius: 3px;
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #504F4F;
    letter-spacing: 0;
    text-align: center;
}

.orderDetail-wrap .showStep-wrap {
    background: #FFFFFF;
    border-radius: 3px;
    margin: 0 20rpx;
    padding: 44rpx 32rpx;
}

.orderDetail-wrap .showStep-wrap .left-wrap {
    width: 50rpx;
}

.orderDetail-wrap .showStep-wrap .left-wrap .icon-font {
    font-size: 30rpx;
}

.orderDetail-wrap .showStep-wrap .center-wrap {
    font-family: PingFangSC-Regular;
    line-height: 40rpx;
    font-size: 28rpx;
    color: #3E3D3E;
    letter-spacing: 0;
    justify-content: flex-start;
    width: calc(100% - 82rpx);
    padding: 0 20rpx;
}

.orderDetail-wrap .showStep-wrap .right-wrap .icon-font {
    font-size: 22rpx;
}

.orderDetail-wrap .user-info-wrap {
    margin: 0 20rpx;
    padding: 40rpx;
    background: #FFFFFF;
    border-radius: 3px;
}

.orderDetail-wrap .user-info-wrap .user-info-top {
    font-family: PingFangSC-Regular;
    line-height: 40rpx;
    font-size: 28rpx;
    color: #3E3D3E;
    letter-spacing: 0;
    justify-content: flex-start;
}

.orderDetail-wrap .user-info-wrap .user-info-top .top-icon-w {
    width: 32rpx;
}

.orderDetail-wrap .user-info-wrap .user-info-top .top-data-w {
    width: calc(100% - 32rpx);
}

.orderDetail-wrap .user-info-wrap .user-info-top .top-data-w .data-name {
    padding: 0 38rpx;
}

.orderDetail-wrap .user-info-wrap .user-info-address-w {
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #A4A3A4;
    letter-spacing: 0;
    margin-left: 70rpx;
}

.orderDetail-wrap .user-info-wrap .user-info-address-w .address-top {
    margin-top: 30rpx;
}

.orderDetail-wrap .user-info-wrap .user-info-address-w .address-bottom {
    margin-top: 8rpx;
}

.orderDetail-wrap .goods-info-wrap {
    background: #FFFFFF;
    border-radius: 3px;
    margin: 0 20rpx;
    padding-left: 30rpx;
}

.orderDetail-wrap .goods-info-wrap .freebie-item {
    display: flex;
    padding: 40rpx 30rpx 40rpx 0;
    justify-content: space-between;
    border-bottom: 2rpx solid #F2F2F2;
}

.orderDetail-wrap .goods-info-wrap .freebie-item .left {
    display: flex;
}

.orderDetail-wrap .goods-info-wrap .freebie-item .img {
    width: 100rpx;
    height: 100rpx;
    display: block;
    background-color: #E8E8E8;
}

.orderDetail-wrap .goods-info-wrap .freebie-item .img image {
    width: 100rpx;
    height: 100rpx;
}

.orderDetail-wrap .goods-info-wrap .freebie-item .freebie {
    display: flex;
}

.orderDetail-wrap .goods-info-wrap .freebie-item .freebie .tag {
    height: 30rpx;
    line-height: 26rpx;
    font-size: 20rpx;
    border-radius: 3px;
    color: #092DC5;
    padding: 0 6rpx;
    border: 2rpx solid #092dc5;
    box-sizing: border-box;
    align-items: center;
    margin-right: 6rpx;
}

.orderDetail-wrap .goods-info-wrap .freebie-item .freebie .txt {
    font-size: 24rpx;
    font-family: PingFangSC-Regular,PingFangSC;
    font-weight: 400;
    color: #3e3d3e;
    margin-top: -2rpx;
    padding-left: 20rpx;
    width: 440rpx;
    line-height: 40rpx;
}

.orderDetail-wrap .goods-info-wrap .freebie-item .num {
    font-size: 24rpx;
    font-family: PingFangSC-Regular,PingFangSC;
    font-weight: 400;
    color: #3e3d3e;
}

.orderDetail-wrap .goods-info-wrap .goods-wrap {
    padding: 0 32rpx 0 0;
}

.orderDetail-wrap .goods-info-wrap .goods-wrap .goods-img-w {
    width: 160rpx;
    height: 160rpx;
    margin: 32rpx 0;
}

.orderDetail-wrap .goods-info-wrap .goods-wrap .goods-img-w image {
    width: 160rpx;
    height: 160rpx;
    background-color: #E8E8E8;
}

.orderDetail-wrap .goods-info-wrap .goods-wrap .goods-info {
    width: calc(100% - 190rpx);
    height: 160rpx;
    font-family: PingFangSC-Regular;
    padding-right: 60rpx;
    position: relative;
    justify-content: flex-start;
}

.orderDetail-wrap .goods-info-wrap .goods-wrap .goods-info .goods-name {
    line-height: 40rpx;
    font-size: 28rpx;
    color: #3E3D3E;
    letter-spacing: 0;
}

.orderDetail-wrap .goods-info-wrap .goods-wrap .goods-info .advance {
    position: absolute;
    top: 0;
    right: 0;
    font-size: 24rpx;
    color: #092DC5;
}

.orderDetail-wrap .goods-info-wrap .goods-wrap .goods-info .goods-size {
    margin-top: 8rpx;
    font-size: 24rpx;
    line-height: 34rpx;
    color: #A4A3A4;
    letter-spacing: 0;
}

.orderDetail-wrap .goods-info-wrap .goods-wrap .goods-info .good-price-w {
    margin-top: 38rpx;
    line-height: 40rpx;
    font-size: 28rpx;
    color: #3E3D3E;
    letter-spacing: 0;
    position: absolute;
    width: 100%;
    bottom: 0;
}

.orderDetail-wrap .goods-info-wrap .goods-wrap .goods-info .good-price-w .goods-num {
    float: right;
}

.orderDetail-wrap .goods-info-wrap .goods-wrap .space-line {
    height: 2rpx;
    background-color: #F2F2F2;
    width: 100%;
    margin-left: 30rpx;
}

.orderDetail-wrap .goods-info-wrap .gift-wrap {
    padding: 0 32rpx;
    border-top: 2rpx solid #F2F2F2;
}

.orderDetail-wrap .goods-info-wrap .gift-wrap .gift-img-w {
    width: 114rpx;
    height: 114rpx;
    margin: 32rpx 0;
}

.orderDetail-wrap .goods-info-wrap .gift-wrap .gift-img-w image {
    width: 114rpx;
    height: 114rpx;
    border: 1rpx solid #092DC5;
}

.orderDetail-wrap .goods-info-wrap .gift-wrap .gift-info {
    width: calc(100% - 144rpx);
    font-family: PingFangSC-Regular;
}

.orderDetail-wrap .goods-info-wrap .gift-wrap .gift-info .gift-name {
    line-height: 40rpx;
    font-size: 28rpx;
    color: #3E3D3E;
    letter-spacing: 0;
}

.orderDetail-wrap .goods-info-wrap .gift-wrap .gift-info .gift-name .tag {
    border: 2rpx solid #092DC5;
    color: #092DC5;
    height: 30rpx;
    line-height: 30rpx;
    width: 60rpx;
    font-size: 20rpx;
    border-radius: 4rpx;
    float: left;
    text-align: center;
    margin-top: 6rpx;
    margin-right: 14rpx;
}

.orderDetail-wrap .goods-info-wrap .gift-wrap .gift-info .gift-size {
    margin-top: 6rpx;
    font-size: 24rpx;
    line-height: 34rpx;
    color: #A4A3A4;
    letter-spacing: 0;
}

.orderDetail-wrap .goods-info-wrap .gift-wrap .gift-info .good-price-w {
    margin-top: 6rpx;
    line-height: 34rpx;
    font-size: 24rpx;
    color: #A4A3A4;
    letter-spacing: 0;
}

.orderDetail-wrap .goods-info-wrap .gift-wrap .gift-info .good-price-w .gift-num {
    float: right;
}

.orderDetail-wrap .goods-total-wrap {
    background: #FFFFFF;
    border-radius: 3px;
    margin: 0 20rpx;
    padding: 0 32rpx;
}

.orderDetail-wrap .goods-total-wrap.space-margin {
    margin: 20rpx;
}

.orderDetail-wrap .goods-total-wrap .num-wrap {
    padding: 8rpx 0 32rpx 0;
}

.orderDetail-wrap .goods-total-wrap .num-wrap .item-wrap {
    line-height: 40rpx;
    font-family: PingFangSC-Regular;
    letter-spacing: 0;
    font-size: 28rpx;
    margin-top: 32rpx;
}

.orderDetail-wrap .goods-total-wrap .num-wrap .item-wrap .item-left {
    color: #3E3D3E;
}

.orderDetail-wrap .goods-total-wrap .num-wrap .item-wrap .item-right {
    color: #A4A3A4;
    text-align: right;
}

.orderDetail-wrap .goods-total-wrap .total-price-wrap {
    border-top: 2rpx solid #F2F2F2;
    padding: 32rpx 0;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    line-height: 40rpx;
    color: #3E3D3E;
    letter-spacing: 0;
}

.orderDetail-wrap .goods-total-wrap .show-bill {
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    border-top: 2rpx solid #F2F2F2;
    justify-content: center;
}

.orderDetail-wrap .modal-bottom-close.icon-font {
    font-size: 80rpx;
    color: #FFFFFF;
}

.orderDetail-wrap .orderDetail-btn-wrap {
    position: fixed;
    width: 100%;
    height: 116rpx;
    background-color: #F2F2F2;
    bottom: 0px;
    padding: 20rpx;
}

.orderDetail-wrap .orderDetail-btn-wrap button .icon-font {
    padding-right: 19rpx;
}

.orderDetail-wrap .order-steps-box .step-top {
    height: 178rpx;
    background-color: #FFFFFF;
}

.orderDetail-wrap .order-steps-box .step-top .top-icon-wrap {
    width: 110rpx;
    text-align: center;
}

.orderDetail-wrap .order-steps-box .step-top .top-icon-wrap .icon-font {
    font-size: 40rpx;
}

.orderDetail-wrap .order-steps-box .step-top .top-text-wrap {
    width: 560rpx;
}

.orderDetail-wrap .steps-scroll-w {
    max-height: 800rpx;
}

.orderDetail-wrap .steps-scroll-w .express-noData {
    background-color: #F7F7F7;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    padding: 60rpx 0;
    color: #7E7D7E;
    letter-spacing: 0;
    text-align: center;
}

.orderDetail-wrap .close-steps-btn-wrap {
    position: fixed;
    width: 100%;
    text-align: center;
    z-index: 999;
    bottom: -120rpx;
    color: #FFFFFF;
}

.orderDetail-wrap .close-steps-btn-wrap .icon-font {
    font-size: 80rpx;
}

.orderDetail-wrap .ben-step {
    padding: 20rpx 0;
}

.modal-con {
    padding: 80rpx 0;
    font-family: PingFangSC-Regular;
    font-size: 32rpx;
    color: #3E3D3E;
    letter-spacing: 0;
    text-align: center;
    line-height: 44rpx;
}